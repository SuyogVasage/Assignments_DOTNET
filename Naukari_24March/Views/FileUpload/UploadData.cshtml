@model Naukari_24March.Models.Upload
@{
    ViewData["Title"] = "UploadData";
}

<h1>@ViewBag.UploadStatus</h1>
<form asp-action="UploadData">
     <div class="container">
        <b>@Model.UploadStatus</b> <br>
         Your Candidate ID = <b>@ViewBag.ID</b>
         Please Keep it for Reference.
     </div>
<a asp-action="Index" asp-controller="Home">Finish</a>
</form>  
